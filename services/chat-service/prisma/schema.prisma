generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Message {
  id         Int      @id @default(autoincrement())
  senderId   Int
  receiverId Int?
  roomId     String?
  content    String
  createdAt  DateTime @default(now())
}

